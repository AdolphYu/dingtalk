{
    "name": "mingyoung/dingtalk",
    "description": "EasyDingTalk, Alibaba Dingtalk SDK for PHP",
    "keywords": [
        "dingtalk",
        "dingding",
        "easydingtalk",
        "sdk",
        "钉钉"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ming Young",
            "email": "mingyoungcheung@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "psr/simple-cache": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "EasyDingTalk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EasyDingTalk\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
